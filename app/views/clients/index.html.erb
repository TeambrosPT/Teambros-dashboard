<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Fullname</th>
      <th>Lastname</th>
      <th>Email</th>
      <th>Assignedtrainer</th>
      <th>Contact</th>
      <th>Othernumber</th>
      <th>Address</th>
      <th>Birthday</th>
      <th>Gender</th>
      <th>Clientstatus</th>
      <th>Sessioncost</th>
      <th>Occupation</th>
      <th>Notes</th>
      <th>Emergencycontact</th>
      <th>Imageurl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.client_id %></td>
        <td><%= client.fullName %></td>
        <td><%= client.lastName %></td>
        <td><%= client.email %></td>
        <td><%= client.assignedTrainer %></td>
        <td><%= client.contact %></td>
        <td><%= client.otherNumber %></td>
        <td><%= client.address %></td>
        <td><%= client.birthday %></td>
        <td><%= client.gender %></td>
        <td><%= client.clientStatus %></td>
        <td><%= client.sessionCost %></td>
        <td><%= client.occupation %></td>
        <td><%= client.notes %></td>
        <td><%= client.emergencyContact %></td>
        <td><%= client.imageUrl %></td>
        <td><%= link_to 'Show', client %></td>
        <td><%= link_to 'Edit', edit_client_path(client) %></td>
        <td><%= link_to 'Destroy', client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client', new_client_path %>
